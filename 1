#include <iostream>
using namespace std;
int main()
{
    int N, i, b, c=0;
    cin >> N;
    int A[N];
    int B[N];
    for (i = 0; i < N; i++) {
        cin >> A[i];
    }
    B[0] = A[0];                           
    for (i = 0; i < N; i++) {                      
        if (A[i] != A[i + 1]) {
                B[c] = A[i];
                c++;
        }
    }
        for (i = 0; i < c; i++) {
        cout<< B[i];
    }
}
